{
	"name": "contact-us",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently \"npm run vite\" \"npm run flask\"",
		"vite": "vite dev",
		"flask": "python app.py",
		"build": "npm run vite-build && npm run package",
		"vite-build": "vite build",
		"package": "svelte-kit sync && svelte-package && publint",
		"preview": "vite preview",
		"prepublishOnly": "npm run package"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/package": "^2.0.0",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.2",
		"postcss": "^8.4.27",
		"publint": "^0.1.9",
		"svelte": "^4.0.5",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
